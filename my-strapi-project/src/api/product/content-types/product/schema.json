{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "media": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "shared.seo",
        "shared.rich-text",
        "shared.media"
      ]
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "pricing": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::pricing.pricing",
      "inversedBy": "products"
    },
    "inventory": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::inventory.inventory"
    },
    "product_organization": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::product-organization.product-organization"
    },
    "shipping": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::shipping.shipping",
      "inversedBy": "products"
    },
    "carts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::cart.cart",
      "inversedBy": "products"
    }
  }
}
